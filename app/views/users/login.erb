<h5>Log In To Your BucketList Account:</h5>
<% if flash.has?(:message) %>
  <h6 class="flash"><%= flash[:message] %></h6>
<% end %>
<form action="/login" method="POST">
  <h6><label for="username">Username: </label>
  <input type="text" id="username" name="username"></h6>
  <h6><label for="password">Password: </label>
  <input type="password" id="password" name="password"></h6>
  <input class="submit" type="submit" id="log_in" value="Log In">
</form>
